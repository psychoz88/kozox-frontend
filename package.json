    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^9.0.0",
    "prettier": "3.0.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
